<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= post.description %>
  </p>
  <p>
    <%= image_tag(post.image, style: "width: 200px; height: 200px") %>
  </p>
  <video width="" height="" controls>
    <source src="<%= url_for(post.video) %>" type="video/mp4">
  </video>
</div>
